{
    border-right: 5px solid #fff !important;
    background:#999;
    color:#fff;
    text-align:center;
}
.btmGrid .rgHeaderDiv .left, .rightPane .rgHeaderDiv .left, .rightPane .rgHeaderDiv .right, .btmGrid .rgHeaderDiv .right, .topGrid .rgHeaderDiv .left, .topGrid .rgHeaderDiv .right
{
    border-top:1px solid #fff;
}
.btmGrid .rgHeaderDiv .left, .topGrid .rgHeaderDiv .left, .rightPane .rgHeaderDiv .left
{
    border-right:1px solid #fff;
}
.campaignresults .btmGrid .rgDataDiv td table td, .campaignresults .topGrid .rgDataDiv td table td 
{
    border-right: 1px solid #ccc;
}
.rightPane .rgDataDiv td 
{
    border-right: 1px solid #ccc !important;
}
.topGrid .headerTh, .btmGrid .headerTh, .rightPane .headerTh
{
    background:#999;
    border-right:1px solid #fff;
    color:#fff;
    text-align:center;
}
