
/* CSS Document - Look and feel - for Theme */
/* GENERAL */

html {
	height:100%;
	margin:0;
	padding:0;
	min-height:100%;
	overflow:hidden;
}

body {
	margin:0; 
	padding: 0;
	text-align: center; 
	height:100%;

}
img {
	border:none;
}
.clearAll {
	clear:both;
	height:0px;
	line-height:0px;
	font-size:0px;
	overflow:hidden;
}
p, h1, h2, h3, h4, iframe, input {
	margin:0;
	padding:0;
	font-weight:normal;
}
td, th, thead
{
	padding:0;
	margin:0;
}
.error 
{
	color:#ff0000;
}
/* MARGIN / PADDING ADJUSTMENTS */

.alignRight, td.alignRight
{
	text-align:right !important;
}
.alignLeft 
{
	text-align:left;
}
.tip 
{
    font-size:10px;
    font-style:italic;
}
/* STRUCTURE */
div.wrapper {
	width: 100%;  
	margin: 0 auto; 
	text-align: left; 
}
div.header {
	padding:8px 16px 0 16px;
}
div.contentWrapper 
{
    position:relative;
}
div.contentArea 
{
    position:relative;
    /*overflow-x:hidden;*/
    overflow-y:auto;
    overflow-x:hidden;
    margin:10px;
    padding:0;
    border:1px solid #ccc;
    overflow:auto;
}
div.footer {
	position: fixed;
	width:100%;
	text-align:center;
	bottom:0px;
	left:0px;
	z-index:200;
}
.ie6 .footer 
{
    position:absolute;
    left:4px;
    bottom:-1px;
}
div.footer .inside{
	padding:6px 4px;
	
}
div.disclaimer 
{
	padding:40px;
}
div.terms 
{
	padding:40px;
}
div.terms p
{
	padding:10px 0;
}

/* NAVIGATION */
div.welcomeMenu {
	float:right;
	padding:4px 14px;
	text-align:right;
}
div.welcomeMenu a {
	text-decoration:none;
	padding:0 10px;
}
div.welcomeMenu a:hover {
	text-decoration:underline;
}
div.logo {
	float:left;
	width:50%;
}
div.logo a { 
	text-indent:-1000px;
	width:300px;
	height:30px;
	display:block;
}
.headerLogo 
{
	float:left;
	margin:0 0 8px 0;
}
.tagLine 
{
	padding:8px 20px;
	color:#fff;
	font-size:11px;
	float:left;
}



.topLeft 
{
    position:absolute;
    left:0px;
    top:0px;
    z-index:100;
}
.topRight 
{
    position:absolute;
    right:0px;
    top:0px;
    z-index:100;
}
.impactStatusTextBox 
{
    text-align:center;
}
/* LOG IN PAGE */
body.login 
{
	overflow:hidden;
}
#spanProgress 
{
	margin:15px 0px;
	display:none;
}
div.signIn {
	width:722px;
	margin: 0 auto;
	position:relative;
}
div.signIn .top{
	position:absolute;
	top:0px;
	left:0px;
}
div.signIn .btm{
}
div.signIn .inside{
	padding:100px 20px 0 20px;
	
}
div.signIn .inside .userIcon {
	float:left;
	padding:11px 2px 0px 35px;
}
div.signIn .logos{
	position:absolute;
	top:-19px;
	left:50px;
    z-index:100;
}
div.signIn .inside .welcome{
	padding-bottom:10px;
	width:250px;
	margin: 0 auto 20px auto;
}
div.signIn .inside .userInfo{
	width:300px;
	float:left
}
.loginBox td 
{
    text-align: left
}
div.signIn .loginBox input
{
	border:none;
}
div.signIn p {
	padding: 5px 0;
	float:left;
}
input.formButton 
{
	background:none;
	border:none;
	display:inline-block;
}
.login .coreTextBox, .modalForm .coreTextBox {
	background-position:0 0;
	padding-left:5px;
	margin-bottom:3px;
}
.login span.readOnlyBox {
	padding-left:2px;
}

.login .coreTextBox, .login .coreTextBox *, .modalForm .coreTextBox, .modalForm .coreTextBox * {
	display:inline-block;
	height:22px;
}
.login .coreTextBox .bg, .modalForm .coreTextBox .bg {
	background-position: top right;
	background-repeat:no-repeat;
	padding-right:5px;
}
.login .coreTextBox .bg2, .modalForm .coreTextBox .bg2 {
	background-position:0 -52px;
	background-repeat:repeat-x;
}
.login .coreTextBox input, .modalForm .coreTextBox input
{
	border:none 0px;
	background:none;
	height:18px;
	width:160px;
}
.login INPUT.textBoxReadOnly 
{
		width:167px;
}
.login .rightCol
{
	height:20px;
}
.mt30 
{
    color: #2d58a7;
    line-height:25px;
    }
    
.titleSelectLeft 
{float:left; text-align:right; width:55%;
}
.titleSelectRight 
{float:right; text-align:left; width:45%; padding-top:5px;
}
