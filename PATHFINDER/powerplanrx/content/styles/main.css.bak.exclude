/* goals tables */
.btmGrid .rgDataDiv .rgRow td, .rightPane .rgDataDiv .rgRow td, .rightPane .rgDataDiv .rgAltRow td, .btmGrid .rgDataDiv .rgAltRow td, .topGrid .rgDataDiv .rgRow td, .topGrid .rgDataDiv .rgAltRow td
{
    padding:0px;
    }
