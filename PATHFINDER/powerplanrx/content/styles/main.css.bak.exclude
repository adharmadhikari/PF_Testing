/* JQUERY UI POP UP CSS OVERIDES */
.ui-widget {
	FONT-FAMILY: Arial !important;
}
.ui-dialog {
	PADDING: 0 !important; 
}

.ui-state-hover {
	BORDER: none !important; 
	BACKGROUND:  none !important; 
}
.ui-widget-overlay  
{
    background: #333 !important; 
    opacity: .5 !important;
    filter:Alpha(Opacity=50) !important; }
.ui-dialog-content 
{
    padding:0px !important;
}
.hiddencolumn
{
    display:none;
}
